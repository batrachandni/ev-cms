{
    "name": "react-app",
    "version": "1.0.0",
    "description": "react-app",
    "main": "index.js",
    "scripts": {
        "start": "npm-run-all --parallel dev:*",
        "dev:server": "nodemon --watch build --exec node build/bundle.js",
        "dev:build-server": "webpack --env.NODE_ENV=development --mode=development --config webpack.config.server.js --watch",
        "dev:build-client": "webpack --env.NODE_ENV=development --mode=development --config webpack.config.client.js --watch",
        "build": "npm-run-all --parallel prod:*",
        "prod:server": "nodemon --watch build --exec node build/bundle.js",
        "prod:build-server": "webpack --env.NODE_ENV=production --mode=production --config webpack.config.server.js --watch",
        "prod:build-client": "webpack --env.NODE_ENV=production --mode=production --config webpack.config.client.js --watch",
        "deploy": "npm run deploy:server && npm run deploy:client",
        "deploy:server": "webpack --env.NODE_ENV=production --mode=production --config webpack.config.server.js",
        "deploy:client": "webpack --env.NODE_ENV=production --mode=production --config webpack.config.client.js"
    },
    "author": "Studio Graphene",
    "license": "ISC",
